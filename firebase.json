{
  "hosting": [
    {
      "target": "production",
      "public": "public",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
      
    },
    {
      "target": "devenv",
      "public": "public",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
     
    },
    {
      "target": "staging",
      "public": "public",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
      
    }
  ],
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ],
    "source": "functions"
  }
}